<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style TargetType="Button" x:Key="btnPink_ResourceFile">
        <Setter Property="Width" Value="300"></Setter>
        <Setter Property="Background" Value="Pink"></Setter>
    </Style>
    <LinearGradientBrush x:Key="LinearBrush"  EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="Black"/>
        <GradientStop Color="#FF48CEB5" Offset="1"/>
        <GradientStop Color="#FF091916" Offset="0"/>
        <GradientStop Color="#FFE1DB57" Offset="0"/>
        <GradientStop Color="#FF99D482" Offset="1"/>
        <GradientStop Color="#FF9AD481" Offset="0.688"/>
    </LinearGradientBrush>
</ResourceDictionary>